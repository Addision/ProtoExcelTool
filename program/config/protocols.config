<?xml version="1.0" encoding="GB2312"?>
 <protocols>
 	<module name="11 login" package="">
 		<protocol type="1" id="1101" name="CreateAccountReq" desc="客户端请求创建账号" content="string Account = 1; //账号&amp;#xA;string Password = 2; // md5之后的密码-前端要做一个简单验证"/>
 		<protocol type="2" id="1108" name="SelectRoleAck" desc="选择角色返回" content="int32 ErrCode = 1; // 只有一个错误返回"/>
 		<protocol type="2" id="1110" name="DeleteRoleAck" desc="删除角色返回" content="int32 ErrCode = 1;"/>
 		<protocol type="2" id="1104" name="LoginAppAck" desc="服务器返回请求登录信息" content="int32 ErrCode = 1; // 返回码&amp;#xA;repeated RoleInfo RoleList = 2; // 角色列表"/>
 		<protocol type="2" id="1102" name="CreateAccountAck" desc="创建账号返回" content="string Token=1; // 以后会作为唯一一个登录凭证&amp;#xA;int64 AccountId=2; // 某个账号的唯一id"/>
 		<protocol type="1" id="1103" name="LoginAppReq" desc="客户端登录请求" content="string Token = 1; // 登录验证token&amp;#xA;int64 AccountId = 2; // 账号id&amp;#xA;"/>
 		<protocol type="1" id="1105" name="CreateRoleReq" desc="创建角色请求" content="int64 AccountId = 1; // 账号id&amp;#xA;int32 RoleConfId = 2; // 选择创建角色id&amp;#xA;string RoleName = 3; // 创建角色名字"/>
 		<protocol type="4" id="" name="RoleInfo" desc="角色信息" content="int64 UserId = 1; // 角色唯一id，服务器端生成的&amp;#xA;int32 RoleImageId = 2; // 角色形象id 一般就是1、2、3、4四种角色&amp;#xA;string RoleName = 3; // 角色名字&amp;#xA;int32 RoleLevel = 4; // 角色等级&amp;#xA;"/>
 		<protocol type="1" id="1107" name="SelectRoleReq" desc="角色选择请求" content="int64 RoleId = 1; // 选择某个角色进入游戏"/>
 		<protocol type="1" id="1109" name="DeleteRoleReq" desc="删除角色" content="int64 RoleId = 1; // 删除角色id"/>
 		<protocol type="2" id="1106" name="CreateRoleAck" desc="创建角色返回" content="int32 ErrCode = 1; // 如果成功&amp;#xA;int64 RoleId = 2; // 创建角色的唯一id&amp;#xA;&amp;#xA;"/>
 	</module>
 	<module name="12 chat" package=""/>
 	<module name="20 game" package=""/>
 </protocols>
