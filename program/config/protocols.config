<?xml version="1.0" encoding="GB2312"?>
 <protocols>
 	<module name="11 login" package="package pb;">
 		<protocol type="1" id="1101" name="LoginReq" desc="登录请求" content="string name = 1;&amp;#xA;string password = 2;"/>
 		<protocol type="2" id="1102" name="LoginAck" desc="登录返回" content="int32 ErrCode = 1; // 0 正常返回&amp;#xA;int64 UserId = 2;  // 用户id&amp;#xA;string UserName = 3; // 用户名称"/>
 		<protocol type="1" id="1103" name="GetProjectsReq" desc="获取工程列表" content="int64 UserId = 1;"/>
 		<protocol type="2" id="1104" name="GetProjectsAck" desc="获取工程列表返回" content="int64 Id = 1; &amp;#xA;string IconAddress = 2; &amp;#xA;string Name = 3; &amp;#xA;int64 SaveTime = 4;"/>
 		<protocol type="1" id="1105" name="GetProjectReq" desc="获取某个工程" content="int64 UserId = 1; &amp;#xA;int64 ProjectId = 2;"/>
 		<protocol type="2" id="1106" name="GetProjectAck" desc="获取某个工程返回" content="int64 Id = 1;   // 工程id &amp;#xA;string IconAddress = 2; // 工程缩略图地址 &amp;#xA;string Name = 3; // 工程名称&amp;#xA;int64 SaveTime = 4; &amp;#xA;string ProjectAddress = 5;"/>
 		<protocol type="1" id="1107" name="SaveProjectReq" desc="保存某个工程" content="int64 UserId = 1;&amp;#xA;int64 ProjectId = 2;&amp;#xA;bytes Icon = 3;&amp;#xA;string Name = 4;&amp;#xA;bytes ProjectFile = 5;&amp;#xA;int64 SaveTime = 6;"/>
 		<protocol type="2" id="1108" name="SaveProjectAck" desc="保存工程返回" content="int32 ErrCode = 1;&amp;#xA;int64 UserId = 2;&amp;#xA;string UserName = 3;&amp;#xA;int64 ProjectId = 4;&amp;#xA;string ProjectName = 5;&amp;#xA;"/>
 		<protocol type="2" id="1104" name="GetProjectsAck" desc="获取工程列表返回" content="int32 ErrCode = 1;&amp;#xA;int64 Id = 2; &amp;#xA;string IconAddress = 3; &amp;#xA;string Name = 4; &amp;#xA;int64 SaveTime = 5;"/>
 		<protocol type="2" id="1106" name="GetProjectAck" desc="获取某个工程返回" content="int32 ErrCode = 1;&amp;#xA;int64 Id = 2;   // 工程id &amp;#xA;string IconAddress = 3; // 工程缩略图地址 &amp;#xA;string Name = 4; // 工程名称&amp;#xA;int64 SaveTime = 5; &amp;#xA;string ProjectAddress = 6;"/>
 		<protocol type="1" id="1109" name="GetResourcesReq" desc="获取资源列表" content="int64 UserId = 1;&amp;#xA;int32 SrcType = 2;"/>
 		<protocol type="2" id="1110" name="GetResourcesAck" desc="获取资源列表返回" content="int32 ErrCode = 1;&amp;#xA;repeated SourceInfo sources = 2"/>
 	</module>
 	<module name="00 common" package="package pb;">
 		<protocol type="1" id="1000" name="HeartBeatReq" desc="心跳请求" content=""/>
 		<protocol type="2" id="1001" name="HeartBeatAck" desc="" content=""/>
 		<protocol type="4" id="9001" name="SourceInfo" desc="资源信息" content="int64 ResourceId = 1; // 资源id&amp;#xA;string IconAddr = 2; // 资源下载地址&amp;#xA;string Name = 3; // 资源名称&amp;#xA;string ShortDesc = 4; // 资源描述&amp;#xA;int32 SrcType = 5; // 资源类型"/>
 		<protocol type="4" id="9002" name="Assets" desc="资产信息" content="int64 AssetsId = 1; // 资产id&amp;#xA;string Name = 2; // 名称&amp;#xA;float Price = 3; //&amp;#xA;int32 Discount = 4; // 折扣&amp;#xA;int64 Size = 5; // 资产文件大小&amp;#xA;string Path = 6; // 资产路径&amp;#xA;string Description = 7; // 资产描述&amp;#xA;bytes Assets = 8; // 资产文件&amp;#xA;bytes Icon = 9; // 图标文件&amp;#xA; "/>
 	</module>
 </protocols>
