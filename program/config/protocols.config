 <protocols>
 	<module name="10 login" package="">
 		<protocol type="1" id="7057" name="RoleLoginReq" desc="界面上选中某个角色进入游戏" content="string accout = 1; &amp;#xA;int32 role_id = 2;&amp;#xA;string role_name = 3; "/>
 		<protocol type="2" id="7000" name="RoleLoginAck" desc="角色进入游戏返回消息" content="string accout = 1; &amp;#xA;int32 role_id = 2;&amp;#xA;string role_name = 3;&amp;#xA;int32 scene_id = 4;&amp;#xA;int32 last_scene_id = 5;&amp;#xA;string gw_hostname = 6;&amp;#xA;int32 gw_port = 7;&amp;#xA;int32 gs_index = 8;&amp;#xA;int64 server_time = 9;&amp;#xA;string session_key = 10;&amp;#xA;int32 result = 11;"/>
 		<protocol type="1" id="7150" name="CreateRoleReq" desc="请求创建某个角色" content="string role_name = 1;&amp;#xA;uint32 login_time = 2;&amp;#xA;string login_str = 3;&amp;#xA;uint32 server = 4;&amp;#xA;uint32 anti_wallow = 5;&amp;#xA;uint32 avatar = 6;&amp;#xA;uint32 sex = 7;&amp;#xA;string plat_name = 8;&amp;#xA;string user_uuid = 9;"/>
 		<protocol type="2" id="7100" name="CreateRoleAck" desc="创建角色返回" content="enum RET_TYPE&amp;#xA;{&amp;#xA;RESULT_TYPE_SUCC = 0;// 成功&amp;#xA;RESULT_TYPE_NO_SPACE = 1;// 拥有角色太多，没有空间再创角色&amp;#xA;RESULT_TYPE_EXIST_NAME = 2; // 重名&amp;#xA;RESULT_TYPE_NAME_INVALID = 3; // 名字不合法&amp;#xA;RESULT_TYPE_SERVER_LIMIT = 4; // 服务器禁止创建角色&amp;#xA;};&amp;#xA;int32 errCode = 1;&amp;#xA;uint32 role_id = 2;&amp;#xA;string role_name = 3;&amp;#xA;uint32 avatar = 4;&amp;#xA;uint32 sex = 5;&amp;#xA;int32 create_time = 6;"/>
 		<protocol type="1" id="1050" name="UserEnterSceneReq" desc="玩家进入场景" content="int32 scene_id = 1;&amp;#xA;int32 scene_key = 2;&amp;#xA;int32 last_scene_id = 3;&amp;#xA;int32 role_id = 4;&amp;#xA;string role_name = 5;&amp;#xA;int32 time = 6;&amp;#xA;uint32 is_login = 7;// 登陆还是场景切换&amp;#xA;uint32 reserved1 = 8;&amp;#xA;int32 server = 9;&amp;#xA;string key = 10; &amp;#xA;string user_uuid = 11; &amp;#xA;int32 invitationstatus = 12;"/>
 		<protocol type="2" id="1000" name="UserEnterSceneAck" desc="玩家进入场景返回消息" content="int32 errCode = 1;"/>
 		<protocol type="3" id="7001" name="RoleListNotify" desc="服务器推送角色列表" content="enum LoginAckCode&amp;#xA;{&amp;#xA; LOGIN_RESULT_SUC = 0;&amp;#xA; LOGIN_NO_THREAD = 1100;&amp;#xA; LOGIN_SERVER_ERROR = 1102;&amp;#xA; LOGIN_RESULT_EXIST = 1103;&amp;#xA; LOGIN_SCENE_NOT_EXIST = 1104;&amp;#xA; LOGIN_RESULT_NO_GATEWAY = 1105;&amp;#xA; LOGIN_RESULT_NO_ROLE = 1106;&amp;#xA; LOGIN_THREAD_BUSY = 1107;&amp;#xA; LOGIN_LOGIN_FORBID = 1108;&amp;#xA; LOGIN_ANTI_WALLOW = 1109;&amp;#xA;};&amp;#xA;&amp;#xA;int32 errCode = 1;&amp;#xA;int32 count = 2;&amp;#xA;string user_uuid = 3;&amp;#xA;string wallet = 4;&amp;#xA;int32 invitationstatus = 5;&amp;#xA;repeated RoleItem roleItem= 6;"/>
 		<protocol type="3" id="1100" name="EnterSceneNotify" desc="进入场景广播消息" content="int32 sceneid = 1;&amp;#xA;int32 obj_id = 2;&amp;#xA;int32 open_line = 3;&amp;#xA;float pos_x = 4;&amp;#xA;float pos_y = 5;  &amp;#xA;int32 hp = 6;&amp;#xA;int32 max_hp = 7;&amp;#xA;int32 scene_key = 8;&amp;#xA;float height = 9;&amp;#xA;float pos_z = 10; "/>
 		<protocol type="3" id="1101" name="LeaveSceneNotify" desc="玩家离开场景" content="int32 sceneid = 1;"/>
 		<protocol type="3" id="1104" name="ObjMoveNotify" desc="广播移动消息" content="float dir = 1;&amp;#xA;float speed_x = 2;&amp;#xA;float speed_y = 3;&amp;#xA;float speed_z = 4;&amp;#xA;float pos_x = 5;&amp;#xA;float pos_y = 6;&amp;#xA;float pos_z = 7;&amp;#xA;float rotation_x = 8;&amp;#xA;float rotation_y = 9;&amp;#xA;float rotation_z = 10; &amp;#xA;int32 reserved = 11;&amp;#xA;int32 obj_id = 12;&amp;#xA;float distance = 13;&amp;#xA;uint32 move_mode = 14;&amp;#xA;uint32 reserve2 = 15;&amp;#xA;float height = 16;&amp;#xA;float acceleration_x = 17;&amp;#xA;float acceleration_y = 18;&amp;#xA;float acceleration_z = 19;&amp;#xA;float controlRotation_x = 20;&amp;#xA;float controlRotation_y = 21;&amp;#xA;float controlRotation_z = 22;"/>
 		<protocol type="1" id="1150" name="ObjMoveReq" desc="玩家移动请求" content="float dir = 1;&amp;#xA;float speed_x = 2;&amp;#xA;float speed_y = 3;&amp;#xA;float speed_z = 4;&amp;#xA;float pos_x = 5;&amp;#xA;float pos_y = 6;&amp;#xA;float pos_z = 7;&amp;#xA;float rotation_x = 8;&amp;#xA;float rotation_y = 9;&amp;#xA;float rotation_z = 10;&amp;#xA;float distance = 11;&amp;#xA;float height = 12; &amp;#xA;float acceleration_x = 13;&amp;#xA;float acceleration_y = 14;&amp;#xA;float acceleration_z = 15;&amp;#xA;float controlRotation_x = 16;&amp;#xA;float controlRotation_y = 17;&amp;#xA;float controlRotation_z = 18;"/>
 		<protocol type="3" id="1102" name="VisibleObjEnterRoleNotify" desc="进入视野广播" content="float dir = 1;&amp;#xA;uint32 move_mode_param = 2;&amp;#xA;uint32 role_status = 3;&amp;#xA;uint32 obj_id = 4;&amp;#xA;int32 role_id = 5;&amp;#xA;string role_name = 6; &amp;#xA;int32 hp = 7;&amp;#xA;int32 max_hp = 8;&amp;#xA;uint32 level = 9;&amp;#xA;uint32 sex = 10;&amp;#xA;uint32 speed = 11;&amp;#xA;float pos_x = 12;&amp;#xA;float pos_y = 13;&amp;#xA;float distance = 14;&amp;#xA;uint32 name_color = 15;&amp;#xA;uint32 move_mode = 16;&amp;#xA;uint32 avatar = 17;&amp;#xA;float pos_z = 18;&amp;#xA;float   rotation_x = 19;&amp;#xA;float   rotation_y = 20;&amp;#xA;float    rotation_z = 21;"/>
 		<protocol type="4" id="" name="RoleItem" desc="角色信息" content="int32 role_id = 1;   &amp;#xA;string role_name = 2;&amp;#xA;int32 avatar = 3;&amp;#xA;int32  sex = 4;   &amp;#xA;uint32 create_time = 5;&amp;#xA;uint32 last_login_time  = 6;"/>
 		<protocol type="1" id="7056" name="LoginMsgReq" desc="登录游戏" content="string name = 1; &amp;#xA;string token = 2;&amp;#xA;string uuid = 3;&amp;#xA;string wallet = 4;&amp;#xA;int32 invitationstatus = 5;"/>
 	</module>
 	<module name="20 scene" package="">
 		<protocol type="1" id="3101" name="RoleJumpReq" desc="角色跳跃" content="float dir = 1;&amp;#xA;int32 pos_x = 2;&amp;#xA;int32 pos_y = 3; &amp;#xA;float height = 4;"/>
 		<protocol type="2" id="3102" name="RoleJumpAck" desc="角色跳跃返回" content="int32 obj_id = 1;&amp;#xA;float dir = 2;&amp;#xA;int32 pos_x = 3;&amp;#xA;int32 pos_y = 4; &amp;#xA;float height = 5;"/>
 		<protocol type="3" id="1302" name="RoleReAliveNotify" desc="角色重生" content="int32 obj_id = 1; &amp;#xA;float pos_x = 2;&amp;#xA;float pos_y = 3; &amp;#xA;float pos_z = 4; "/>
 		<protocol type="1" id="8150" name="ChannelChatReq" desc="聊天频道请求" content="int32 content_type = 1;   //CHAT_CONTENT_TYPE_TEXT,CHAT_CONTENT_TYPE_AUDIO,&amp;#xA;int32 from_type = 2; // 发送类型 &amp;#xA;int32 channel_type = 3;// 0：世界，1：阵营，2：场景，3：组队，4：仙盟 &amp;#xA;string content = 4;  // 消息内容"/>
 		<protocol type="2" id="8100" name="ChannelChatAck" desc="频道请求返回" content="int32 from_uid = 1; &amp;#xA;string username = 2; &amp;#xA;int32 sex = 3;&amp;#xA;int32 content_type = 4;  &amp;#xA;int32 channel_type = 5;&amp;#xA;int32 from_type = 6; // 发送类型 &amp;#xA;string content = 7;  // 消息内容"/>
 		<protocol type="3" id="8161" name="ActivityReadyNotify" desc="活动准备" content="int32 activityId = 1;&amp;#xA;int32 count_down = 2;&amp;#xA;int32 switch_type = 3; // 场景切换类型1本地 2跳转远端根据ip,port连接"/>
 		<protocol type="3" id="8162" name="ActivityStartNotify" desc="活动开始" content="int32 activityId = 1;&amp;#xA;int32 switch_type = 2; // 场景切换类型1本地 2跳转远端根据ip,port连接 &amp;#xA;string Ip = 3;&amp;#xA;int32 port = 4;&amp;#xA;int32  maze_seed = 5; // 迷宫种子&amp;#xA;float   town_pos_x = 6; // 出生点x&amp;#xA;float  town_pos_y = 7; // 出生点y&amp;#xA;float   town_pos_z = 8; // 出生点Z"/>
 		<protocol type="1" id="8163" name="JoinActivityReq" desc="加入活动请求" content="int32 activityId = 1;&amp;#xA;int32 join_status = 2;  // 1 --  参加   2---退出 "/>
 		<protocol type="2" id="8164" name="JoinActivityAck" desc="加入活动请求返回" content="int32 activityId = 1;&amp;#xA;int32 errocode = 2;  // 0-- 成功   &gt; 0 ---失败 "/>
 		<protocol type="1" id="8200" name="MazeGetGemReq" desc="迷宫获取宝石请求" content=""/>
 		<protocol type="3" id="9100" name="AreaInitNotify" desc="地块初始化信息" content="int32    errCode = 1;&amp;#xA;int32    count = 2;   &amp;#xA;repeated AreaItem areaItem= 3;"/>
 		<protocol type="4" id="" name="AreaItem" desc="地块信息" content="string area_id = 1;&amp;#xA;int32 role_id = 2;         &amp;#xA;string   user_uuid = 3;"/>
 		<protocol type="3" id="9101" name="AreaTradeNotify" desc="地块交易推送" content="string area_id = 1;&amp;#xA;int32 role_id = 2;    &amp;#xA;string   user_uuid = 3; "/>
 		<protocol type="1" id="9051" name="ServerTimeReq" desc="请求获取服务器时间" content=""/>
 		<protocol type="2" id="9001" name="ServerTimeAck" desc="服务器时间获取返回" content="int32 server_time = 1;&amp;#xA;int32 server_real_start_time = 2;&amp;#xA;int32 open_days = 3;&amp;#xA;int32 server_real_combine_time = 4;"/>
 		<protocol type="1" id="1052" name="TransportReq" desc="传送请求" content="int32 transport_index = 1;"/>
 		<protocol type="1" id="8101" name="SwitchSceneReq" desc="切换场景请求" content="int32 scene_id = 1;"/>
 	</module>
